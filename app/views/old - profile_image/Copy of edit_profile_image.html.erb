<h1><%= @title %></h1>

<%= error_messages_for :profile_image %>

<% form_for(:profile_image, :url => profile_image_path, 
                      :html => { :multipart => true }) do |f| -%>
  <p>
    <label for="mugshot">Upload A Profile Image:</label>
    <%= f.file_field :uploaded_data %>
  </p>
  <p>
    <%= submit_tag 'Update Image' %>
  </p>
<% end -%>



