<h1><%= @title %></h1>


<%= render :partial => 'profile_images/profile_image', :collection =>  [@user], :as => :user%> 

<h1>Edit Image</h1>

<% form_for  @user, :url => profile_image_path,  :html => { :multipart => true } do |f| %>
  
   <p>
    <label for="profile_images">Choose profile image to upload:</label>
    
    <%= f.file_field :avatar %>
    <!--%= f.hidden_field :avatar_cache %-->
  </p>
     
  <div class="actions">
    <%= f.submit "Upload Image" %>
  </div>
<% end %>

