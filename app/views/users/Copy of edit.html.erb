<% end %>

<% form_for :user, @user, :url => user_path,  :html => { :multipart => true } do |f| %>
  
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= current_account.email %>
  </div>
  
<% end %>
      
   
